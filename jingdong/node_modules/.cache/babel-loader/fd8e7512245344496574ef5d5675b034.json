AAA,E;;AAAA,G,iBACS,MAAA,CAAA,K,GAIP,CAPN,CAQI,CAzCN,CA0CE,CApDN,C","sourcesContent":["<template>\r\n  <div class=\"wreap\">\r\n      <div class=\"head\">\r\n        <span\r\n        @click=\"handBack\"\r\n         class=\"iconfont \">&#xe7e0;</span>\r\n        <span class=\"head__text\">新建收货地址</span>\r\n        <span\r\n        @click=\"creatAddress\"\r\n        class=\"head__address\">保存</span>\r\n      </div>\r\n      <div class=\"body__bar\">\r\n        <div class=\"body__item\">\r\n            <span>所在城市:</span>\r\n            <input\r\n            class=\"body__item__text\"\r\n            v-model=\"city\"\r\n            placeholder=\"北京市\"\r\n             type=\"text\">\r\n        </div>\r\n        <div class=\"body__item\">\r\n            <span>小区/大厦/学校:</span>\r\n            <input\r\n            v-model=\"department\"\r\n            class=\"body__item__text\"\r\n            placeholder=\"西二旗小区\"\r\n             type=\"text\">\r\n        </div>\r\n        <div class=\"body__item\">\r\n            <span>楼号-门牌号:</span>\r\n            <input\r\n            v-model=\"houseNumber\"\r\n            class=\"body__item__text\"\r\n            placeholder=\"10号楼1单元\"\r\n             type=\"text\">\r\n        </div>\r\n        <div class=\"body__item\">\r\n            <span>收货人:</span>\r\n            <input\r\n            v-model=\"name\"\r\n            class=\"body__item__text\"\r\n            placeholder=\"双越\"\r\n             type=\"text\">\r\n        </div>\r\n        <div class=\"body__item\">\r\n            <span>联系电话:</span>\r\n            <input\r\n            v-model=\"phone\"\r\n            class=\"body__item__text\"\r\n            placeholder=\"1555666\"\r\n             type=\"text\">\r\n        </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { reactive, toRefs } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { post } from '../../utils/request'\r\nconst useGetAddressList = () => {\r\n  const date = reactive({\r\n    city: '',\r\n    department: '',\r\n    houseNumber: '',\r\n    name: '',\r\n    phone: ''\r\n  })\r\n  const creatAddress = async () => {\r\n    try {\r\n      const result = await post('/api/user/address', {\r\n        city: date.city,\r\n        department: date.department,\r\n        houseNumber: date.houseNumber,\r\n        name: date.name,\r\n        phone: date.phone\r\n      })\r\n      console.log(result)\r\n    } catch (e) {\r\n      console.log('新建地址请求失败')\r\n    }\r\n  }\r\n  const { city, department, houseNumber, name, phone } = toRefs(date)\r\n  return { creatAddress, city, department, houseNumber, name, phone }\r\n}\r\n\r\nconst useTransform = () => {\r\n  const router = useRouter()\r\n  const handBack = () => {\r\n    router.back()\r\n  }\r\n\r\n  return { handBack }\r\n}\r\nexport default {\r\n  name: 'manageAddress',\r\n  setup () {\r\n    const { creatAddress, city, department, houseNumber, name, phone } = useGetAddressList()\r\n    const { handBack } = useTransform()\r\n    return { handBack, creatAddress, city, department, houseNumber, name, phone }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.wreap{\r\n  padding: 0 .2rem;\r\n}\r\n.head{\r\n  margin-bottom: .3rem;\r\n  background: #fff;\r\n  height: .4rem;\r\n  line-height: .4rem;\r\n  box-sizing: border-box;\r\n  box-shadow: 0 8px 16px 0 rgba(0,0,0,0.08);\r\n  display: flex;\r\n  &__text{\r\n    text-align: center;\r\n    flex: 1;\r\n  }\r\n}\r\n.body__bar{\r\n  padding: 0 .2rem;\r\n  line-height: .4rem;\r\n  background: #fff;\r\n  box-shadow: 0 8px 16px 0 rgba(0,0,0,0.08);\r\n\r\n}\r\n.body__item{\r\n  height: .4rem;\r\n  border-bottom: .01rem solid #F1F1F1;\r\n    &__text{\r\n      border-style: none;\r\n      outline-style: none;\r\n      margin-left: .2rem;\r\n    }\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":""}]}