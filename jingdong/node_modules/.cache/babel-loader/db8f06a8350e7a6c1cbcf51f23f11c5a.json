AAA,CAAA,YAAA,CAAa,KAAf,CAAhC,EAAoD;AAAA;AAApD,GAIE,EAHF,mBAAA,CAEW,KAFX,EAEW;AAFN,IAAA,KAAK,EAAC,YAEA;AADV,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAS,MAAA,CAAA,iBAAA,CAAiB,IAAjB,CAAT;AAAA,KAAA;AACK,GAFX,EAEC,MAFD,CAGE,CALN,C,kBAMA,mBAAA,CAiBM,KAjBN,EAiBM;AAhBL,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAS,MAAA,CAAA,iBAAA,CAAiB,KAAjB,CAAT;AAAA,KAAA,CAgBA;AAfN,IAAA,KAAK,EAAC;AAeA,GAjBN,E,CAGI,mBAAA,CAaM,KAbN,EAaM;AAbD,IAAA,KAAK,EAAC,eAaL;AAZL,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAN,YAAA,CAAW,CAAL,EAAK,CAAA,MAAA,CAAL,CAAA;AAYA,GAbN,E,CAGE,U,EACA,U,EACA,mBAAA,CAOM,KAPN,EAAA,UAAA,EAOM,CANJ,mBAAA,CAEqC,KAFrC,EAEqC;AADpC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAQ,MAAA,CAAA,kBAAA,CAAkB,IAAlB,CAAR;AAAA,KAAA,CAC+B;AAArC,IAAA,KAAK,EAAC;AAA+B,GAFrC,EAE2B,MAF3B,CAMI,EAHJ,mBAAA,CAEgE,KAFhE,EAEgE;AAD/D,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAQ,MAAA,CAAA,kBAAA,CAAkB,KAAlB,CAAR;AAAA,KAAA,CAC0D;AAAhE,IAAA,KAAK,EAAC;AAA0D,GAFhE,EAEsD,MAFtD,CAGI,CAPN,C,CALF,C,CAHJ,E;;AAAA,G,YAEqB,MAAA,CAAA,W","sourcesContent":["<template>\r\n        <div class=\"order\">\r\n            <div class=\"order__price\">实付金额 ￥{{calculations.price}}</div>\r\n            <div class=\"order__btn\"\r\n            @click=\"() =>  handleSubmitClick(true)\"\r\n            >提交订单</div>\r\n        </div>\r\n        <div\r\n        @click=\"() =>  handleSubmitClick(false)\"\r\n        class=\"mask\" v-show=\"showConfirm\">\r\n            <div class=\"mask__content\"\r\n            @click.stop\r\n            >\r\n              <h3 class=\"mask__content__title\">确认要离开收银台？</h3>\r\n              <p class=\"mask__content__desc\">请尽快完成支付，否则将被取消</p>\r\n              <div class=\"mask__content__btns\">\r\n                <div\r\n                @click=\"() => handleConfirmOrder(true)\"\r\n                class=\"mask__content__btn\">取消订单</div>\r\n                <div\r\n                @click=\"() => handleConfirmOrder(false)\"\r\n                class=\"mask__content__btn mask__content__btn--active\">确认支付</div>\r\n              </div>\r\n            </div>\r\n        </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue'\r\nimport { useCommonCartEffect } from '../../effects/catEffects'\r\nimport { useRoute, useRouter } from 'vue-router'\r\nimport { post } from '../../utils/request'\r\nimport { useStore } from 'vuex'\r\n\r\nconst useMakeOrderEffect = (shopId, shopName, productList) => {\r\n  const router = useRouter()\r\n  const store = useStore()\r\n  const handleConfirmOrder = async (isCanceled) => {\r\n    const products = []\r\n    for (const i in productList.value) {\r\n      const product = productList.value[i]\r\n      products.push({ id: product._id, num: product.count })\r\n    }\r\n    try {\r\n      const result = await post('/api/order', {\r\n        addressId: '619348a43bce719100576bff',\r\n        shopId,\r\n        shopName: shopName.value,\r\n        isCanceled,\r\n        products\r\n      })\r\n      if (result?.data.errno === 0) {\r\n        router.push({ name: 'OrderList' })\r\n        store.commit('clearCartData', { shopId })\r\n      }\r\n    } catch (e) {\r\n      //  提示下单失败\r\n    }\r\n  }\r\n  return { handleConfirmOrder }\r\n}\r\n\r\nconst useShowMaskEffect = () => {\r\n  const showConfirm = ref(false)\r\n\r\n  const handleSubmitClick = (status) => {\r\n    showConfirm.value = status\r\n  }\r\n  return { showConfirm, handleSubmitClick }\r\n}\r\n\r\nexport default {\r\n  name: 'Order',\r\n  setup () {\r\n    const route = useRoute()\r\n    const shopId = route.params.id\r\n    const { calculations, shopName, productList, cartList } = useCommonCartEffect(shopId)\r\n    const { showConfirm, handleSubmitClick } = useShowMaskEffect()\r\n    const { handleConfirmOrder } = useMakeOrderEffect(shopId, shopName, productList)\r\n    return { cartList, handleSubmitClick, showConfirm, calculations, handleConfirmOrder }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.order{\r\n    width: 100%;\r\n    position: fixed;\r\n    left: 0;\r\n    bottom: 0;\r\n    line-height: .5rem;\r\n    height: .5rem;\r\n    background: #fff;\r\n    display: flex;\r\n    &__price{\r\n        margin-left: .24rem;\r\n        flex: 1;\r\n        font-weight: bold;\r\n        font-size: .16rem;\r\n        color: #333333;\r\n    }\r\n    &__btn{\r\n        font-size: .14rem;\r\n        text-align: center;\r\n        width: .98rem;\r\n        background: #4FB0F9;\r\n        color: #fff;\r\n    }\r\n}\r\n.mask{\r\n    z-index: 1;\r\n    position: absolute;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: 0;\r\n    top: 0;\r\n    background: rgba(0,0,0,0.50);\r\n    &__content{\r\n        position: absolute;\r\n        top: 50%;\r\n        left: 50%;\r\n        transform: translate(-50%,-50%);\r\n        width: 3rem;\r\n        height: 1.56rem;\r\n        background: #fff;\r\n        border-radius: .04rem;\r\n        &__title{\r\n            margin: .24rem auto .08rem auto;\r\n            font-size: .18rem;\r\n            color: #333;\r\n            text-align: center;\r\n        }\r\n        &__desc{\r\n             text-align: center;\r\n            margin: 0 auto .24rem auto;\r\n            color: #666;\r\n            font-size: .14rem;\r\n        }\r\n        &__btns{\r\n            margin-left: .47rem;\r\n            display: flex;\r\n        }\r\n        &__btn{\r\n            margin: 0 .12rem 0 .12rem;\r\n            color: #4FB0F9;\r\n            border: 1px solid #4FB0F9;\r\n            width: .8rem;\r\n            text-align: center;\r\n            line-height: .32rem;\r\n            height: .32rem;\r\n            border-radius: .16rem;\r\n            &--active{\r\n                background: #4FB0F9;\r\n                color: #fff;\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}