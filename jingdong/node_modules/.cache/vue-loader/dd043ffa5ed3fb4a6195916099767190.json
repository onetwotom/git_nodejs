active({ item: {} })\r\n  const getItemData = async () => {\r\n    const result = await get(`/api/shop/${route.params.id}`)\r\n    if (result?.data?.errno === 0 && result?.data) {\r\n      data.item = result.data.data\r\n    }\r\n  }\r\n  const { item } = toRefs(data)\r\n  return { getItemData, item }\r\n}\r\nconst useHandleBack = () => {\r\n  const router = useRouter()\r\n  const handleBack = () => {\r\n    router.back()\r\n  }\r\n  return { handleBack }\r\n}\r\nexport default {\r\n  name: 'Shop',\r\n  components: { ShopInfo, Content, Cart },\r\n  setup () {\r\n    const { getItemData, item } = useGetItemData()\r\n    const { handleBack } = useHandleBack()\r\n    getItemData()\r\n    return { item, handleBack }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.search{\r\n  display: flex;\r\n  margin: 0 .18rem .16rem .185rem;\r\n  &--back{\r\n    color: #B6B6B6;\r\n    line-height: .32rem;\r\n    font-size: .28rem;\r\n    margin-right: .162rem;\r\n  }\r\n  &--icon{\r\n    line-height: .32rem;\r\n    font-size: .18rem;\r\n    margin-right: .12rem;\r\n  }\r\n  &__content{\r\n    display: flex;\r\n    height: .32rem;\r\n    flex:1;\r\n    background: #F5F5F5;\r\n    border-radius: .16rem;\r\n  }\r\n  &__input{\r\n    flex: 1;\r\n    border-radius: .16rem;\r\n    font-size: .14rem;\r\n    background: #F5F5F5;\r\n    outline-style: none;\r\n    border-style: none;\r\n  }\r\n}\r\n</style>\r\n"]}]}